<circuit access="internal">

	<fuseaction name="customizeStylesheet">
		<include template="customizeStylesheet.cfm" />
	</fuseaction>

	<fuseaction name="validateStartForm">
		<include template="validateStartForm.cfm" />
	</fuseaction>
	
	<fuseaction name="generateDocBook" >	
		<include template="argumentFunctions.cfm" />			
		<include template="docbook.cfm" />
	</fuseaction>
	
	<fuseaction name="generateEclipseTableOfContents">
		<include template="generateEclipseTableOfContents" />
 	</fuseaction>
	
	<fuseaction name="generateFrames">
		<include template="generateFrames.cfm" />
 	</fuseaction>
	
	<fuseaction name="getDocBook" >		
		<set name="docBookFile" value="#application.fusebox.webrootdirectory#projects#application.directorySeparator##attributes.projectOutputDirectory##application.directorySeparator#cfcdoc.xml" />
		<invoke object="application.cfcdoc" methodcall="getDocBook(docBookFile)" returnvariable="docbookfilecontents" />
	</fuseaction>
	
	<fuseaction name="saveDocBook" >
		<set name="destination" value="#application.fusebox.webrootdirectory#work\cfcdoc.xml" />
		<include template="saveDocBook.cfm" />
	</fuseaction>
			
	<fuseaction name="setup">
		<if condition="not application.initialized">
			<true>
				<do action="m.initialize" />
			</true>
		</if>
	</fuseaction>
	
	<fuseaction name="initialize">
		<include template="initialize.cfm" />
		<instantiate object="application.cfcdoc" class="cfcdoc" arguments="" />		
		<instantiate object="application.fso" class="FileSystemObject" arguments="'',application.directorySeparator" />	
		
		<set name="application.isEvaluationKey" value="false" />
		<set name="application.initialized" value="true" />		
	</fuseaction>
	
</circuit>